<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".EditaUsuario">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textView76"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Edita tus datos..."
            android:textColor="#3B73DF"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView69"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Nombre(s)" />

        <TextView
            android:id="@+id/Nombresedit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:hint="Nombre Completo"
            android:inputType="textPersonName"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView71"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Puesto" />

        <EditText
            android:id="@+id/Puestoedit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:hint="Puesto"
            android:inputType="textPersonName"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/correocomen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Correo Electronico" />

        <EditText
            android:id="@+id/Correoedit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:hint="Correo electronico"
            android:inputType="textPersonName"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView73"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Numero de celular" />

        <EditText
            android:id="@+id/Numeroedit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:hint="Numero de telefono"
            android:inputType="textPersonName"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/Numerodesc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Numero de Cred de elector" />

        <EditText
            android:id="@+id/NumeroGet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/CredencialElector"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="INE Frontal"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/INEfrontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:visibility="gone"
            app:srcCompat="@drawable/image" />

        <ImageButton
            android:id="@+id/SubeINEF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="FotoFrontalINE"
            android:visibility="gone"
            app:srcCompat="@drawable/ic_baseline_drive_folder_upload_24" />

        <TextView
            android:id="@+id/CredencialElectortrasera"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="INE Trasera"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/INEtrasera"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:visibility="gone"
            app:srcCompat="@drawable/image" />

        <ImageButton
            android:id="@+id/SubeINET"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="FotoTraseraINE"
            android:visibility="gone"
            app:srcCompat="@drawable/ic_baseline_drive_folder_upload_24" />

        <TextView
            android:id="@+id/textView75"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Credencial frontal" />

        <ProgressBar
            android:id="@+id/CargaFoto1"
            style="?android:attr/progressBarStyle"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/Frontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:srcCompat="@drawable/image" />

        <ImageButton
            android:id="@+id/imageButton38"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="FotoFrontal"
            app:srcCompat="@drawable/ic_baseline_drive_folder_upload_24" />

        <TextView
            android:id="@+id/TraseraComen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Credencial trasera" />

        <ProgressBar
            android:id="@+id/CargaFoto2"
            style="?android:attr/progressBarStyle"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/Trasera"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:srcCompat="@drawable/image" />

        <ImageButton
            android:id="@+id/SubeTrasera"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="FotoTrasera"
            app:srcCompat="@drawable/ic_baseline_drive_folder_upload_24" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="Actualizando"
                app:srcCompat="@drawable/ic_baseline_check_24" />

            <ImageButton
                android:id="@+id/imageButton30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_baseline_cancel_24" />
        </LinearLayout>


    </LinearLayout>



</ScrollView>